<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

  <ztc1997.hideablenavbar.view.SeekBarPreference
      android:key="pref_navbar_height_port"
      android:title="@string/navbar_height_port_title"
      android:dependency=""
      android:enabled="true"
      android:defaultValue="100"
      monitorBoxEnabled="true"
      minimum="0"
      maximum="150"
      interval="1"
      monitorBoxUnit="%"/>

  <ztc1997.hideablenavbar.view.SeekBarPreference
    android:key="pref_navbar_height_land"
    android:title="@string/navbar_height_land_title"
    android:dependency=""
    android:enabled="true"
    android:defaultValue="100"
    monitorBoxEnabled="true"
    minimum="0"
    maximum="150"
    interval="1"
    monitorBoxUnit="%"/>

  <ztc1997.hideablenavbar.view.SeekBarPreference
      android:key="pref_navbar_width"
      android:title="@string/navbar_width_title"
      android:dependency=""
      android:enabled="true"
      android:defaultValue="100"
      monitorBoxEnabled="true"
      minimum="0"
      maximum="150"
      interval="1"
      monitorBoxUnit="%"/>

</PreferenceScreen>